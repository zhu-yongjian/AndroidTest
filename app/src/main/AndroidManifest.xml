<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/joint"
        android:label="Joint"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity android:name=".liaotianActivity">
            <intent-filter>
                <action android:name="com.example.androidtest.ACTION_LIAOTIAN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".chenggongActivity" />
        <activity android:name=".yuyueActivity" />
        <activity android:name=".consultation_yuyueActivity">
            <intent-filter>
                <action android:name="com.example.androidtest.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".phoneVerificationActivity" />
        <activity android:name=".resetPasswordActivity" />
        <activity android:name=".homepageActivity" />
    </application>

</manifest>